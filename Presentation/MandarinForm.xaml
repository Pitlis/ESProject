<Window x:Class="ESProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="216,243,0,0" VerticalAlignment="Top" Width="75" Click="button_Click">
            <Button.LayoutTransform>
                <TransformGroup>
                    <ScaleTransform CenterX="10"/>
                    <SkewTransform CenterX="10"/>
                    <RotateTransform CenterX="10"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.LayoutTransform>
        </Button>
        <Button x:Name="btnEditSchedule" Content="Открыть расписание" HorizontalAlignment="Left" Margin="51,53,0,0" VerticalAlignment="Top" Width="145" Click="btnEditSchedule_Click"/>
        <Button x:Name="FactorSetting" Content="FactorSetting" HorizontalAlignment="Left" Margin="340,58,0,0" VerticalAlignment="Top" Width="113" Click="FactorSetting_Click"/>
        <Button x:Name="button1" Content="FacultySettings" HorizontalAlignment="Left" Margin="51,104,0,0" VerticalAlignment="Top" Width="135" Click="button1_Click"/>
        <Button x:Name="btn_filedb" Content="Путь к БД" HorizontalAlignment="Left" Margin="340,142,0,0" VerticalAlignment="Top" Width="75" Click="btn_filedb_Click"/>
        <Button x:Name="button2" Content="VIP" HorizontalAlignment="Left" Margin="89,162,0,0" VerticalAlignment="Top" Width="75" Click="button2_Click"/>
        <Button x:Name="button3" Content="Открыть базу (ТЕСТ)" HorizontalAlignment="Left" Margin="340,227,0,0" VerticalAlignment="Top" Width="141" Height="70" Click="button3_Click"/>
        <Button x:Name="button4" HorizontalAlignment="Left" Margin="51,212,0,0" VerticalAlignment="Top" Width="113" Height="36" Click="button4_Click">
            <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="Преподаватель - аудитории"/>
        </Button>
        <Button x:Name="button5" Content="BaseWizard" HorizontalAlignment="Left" Margin="249,142,0,0" VerticalAlignment="Top" Width="75" Click="button5_Click"/>
        <Button x:Name="button6" HorizontalAlignment="Left" Margin="51,261,0,0" VerticalAlignment="Top" Width="113" Height="36" Click="button6_Click">
            <TextBlock x:Name="textBlock1" TextWrapping="Wrap" Text="Преподаватель - корпуса"/>
        </Button>

    </Grid>
</Window>
