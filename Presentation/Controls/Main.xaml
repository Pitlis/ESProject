<UserControl x:Class="Presentation.Controls.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESProject"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        VerticalAlignment="Center" HorizontalAlignment="Center"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="pack://application:,,,/MaterialDesignThemes.Wpf;component/Resources/Roboto/#Roboto">
    <DockPanel>

        <ContentControl>
            <Grid>

                <Button Style="{StaticResource MaterialDesignFloatingActionButton}"
                        Margin="174,300,0,152" HorizontalAlignment="Left"
                        ToolTip="Начать составление расписания"
                        Click="button_Click" RenderTransformOrigin="4.714,2.429" Height="100" Width="100" FontSize="26.667"
                        >Старт
                </Button>
                <Button x:Name="button1" Content="FacultySettings" HorizontalAlignment="Left" Margin="51,32,0,0" VerticalAlignment="Top" Width="135" Click="button1_Click"/>
                <Button x:Name="button2" Content="VIP" HorizontalAlignment="Left" Margin="51,87,0,0" VerticalAlignment="Top" Width="135" Click="button2_Click"/>
                <Button x:Name="button3" Content="Открыть базу (ТЕСТ)" HorizontalAlignment="Left" Margin="240,67,0,0" VerticalAlignment="Top" Width="224" Height="95" Click="button3_Click"/>
                <Button x:Name="button4" HorizontalAlignment="Left" Margin="51,142,0,0" VerticalAlignment="Top" Width="135" Height="44" Click="button4_Click">
                    <TextBlock x:Name="textBlock" TextWrapping="Wrap" Text="Преподаватель - аудитории"/>
                </Button>
                <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="352,337,0,0" VerticalAlignment="Top" Width="76" Click="button_Click_1"/>
            </Grid>
        </ContentControl>
    </DockPanel>
</UserControl>